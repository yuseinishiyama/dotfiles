[user]
        name = nishiyama_y
        email = nishiyama_y@mti.co.jp
[core]
       editor = emacs
       quotepath = false
       autocrlf = false
[color]
        status = auto
        diff = auto
	branch = auto
	interactive = auto
	grep = auto
[alias]
	s   = status
	st  = status
	sh  = show
	c   = commit
	co  = checkout
	br  = branch
	log-graph = log --graph --date=short --pretty=format:'%Cgreen%h %cd %Cblue%cn %Creset%s'
	log-all = log --graph --all --color --pretty='%x09%h %cn%x09%s %Cred%d%Creset'
	di = diff
	sm = submodule