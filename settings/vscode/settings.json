{
  "files.autoSave": "afterDelay",
  "editor.cursorStyle": "block",
  "workbench.colorTheme": "8-Bit Dark",
  "[plaintext]": {},
  "explorer.confirmDragAndDrop": false,
  "explorer.confirmDelete": false,
  "gitlens.keymap": "alternate",
  "files.associations": {
    "*Fastfile": "ruby",
    "Dangerfile": "ruby"
  },
  "gitlens.historyExplorer.enabled": true,
  "gitlens.views.fileHistory.enabled": true,
  "editor.renderControlCharacters": true,
  "files.trimTrailingWhitespace": true,
  "markdown-pdf.displayHeaderFooter": false,
  "markdown-pdf.breaks": true,
  "markdown-pdf.scale": 0.9,
  "markdown-pdf.stylesRelativePathFile": true,
  "markdown-pdf.margin.top": "0.5cm",
  "markdown-pdf.margin.left": "0.5cm",
  "markdown-pdf.margin.right": "0.5cm",
  "markdown-pdf.margin.bottom": "0.5cm",
  "editor.suggestSelection": "first",
  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
  "editor.tabSize": 2,
  "window.newWindowDimensions": "maximized",
  "update.showReleaseNotes": false,
  "gitlens.showWhatsNewAfterUpgrades": false,
  "go.formatTool": "goimports",
  "go.useLanguageServer": true,
  "editor.minimap.enabled": false,
  "breadcrumbs.enabled": true,
  "workbench.statusBar.visible": true,
  "python.languageServer": "Default",
  "python.formatting.provider": "autopep8",
  "python.formatting.autopep8Args": [
    "--indent-size=2"
  ],
  "go.languageServerExperimentalFeatures": {
    "diagnostics": true
  },
  "editor.rulers": [
    120
  ],
  "editor.formatOnSave": true,
  "editor.formatOnPaste": true,
  "editor.cursorBlinking": "smooth",
  "editor.renderWhitespace": "all",
  "workbench.editor.enablePreviewFromQuickOpen": false,
  "workbench.editor.highlightModifiedTabs": true,
  "workbench.editor.tabSizing": "shrink",
  "window.openFilesInNewWindow": "on",
  "window.openFoldersInNewWindow": "on",
  "window.openWithoutArgumentsInNewWindow": "on",
  "workbench.editorAssociations": {
    "*.ipynb": "jupyter.notebook.ipynb"
  },
  "editor.wordWrap": "on",
  "[python]": {
    "editor.tabSize": 2,
    "gitlens.codeLens.symbolScopes": [
      "!Module"
    ],
    "editor.formatOnType": true
  },
  "[terraform]": {
    "editor.formatOnSave": true,
    "editor.defaultFormatter": "hashicorp.terraform"
  },
  "files.insertFinalNewline": true,
  "go.toolsManagement.autoUpdate": true,
  "vs-kubernetes": {
    "vscode-kubernetes.minikube-path.mac": "/Users/yuseinishiyama/.vs-kubernetes/tools/minikube/darwin-amd64/minikube"
  },
  "github.copilot.enable": {
    "*": true,
    "plaintext": true,
    "markdown": false,
    "scminput": false,
    "yaml": false
  },
  "editor.guides.bracketPairs": true,
  "python.testing.pytestEnabled": true,
  "terminal.integrated.scrollback": 100000,
  "editor.bracketPairColorization.enabled": true,
  "window.zoomLevel": 1,
  "editor.inlineSuggest.enabled": true,
  "[ruby]": {
    "editor.defaultFormatter": "Shopify.ruby-lsp",
    "editor.formatOnSave": true,
    "editor.formatOnType": true,
    "editor.tabSize": 2,
    "editor.insertSpaces": true,
    "editor.semanticHighlighting.enabled": true,
    "files.trimTrailingWhitespace": true,
    "files.insertFinalNewline": true,
    "files.trimFinalNewlines": true,
    "editor.rulers": [
      120
    ]
  },
  "cSpell.enableFiletypes": [
    "ruby"
  ],
  "remote.SSH.connectTimeout": 150,
  "explorer.sortOrderLexicographicOptions": "unicode",
  "github.copilot.editor.enableAutoCompletions": true,
  "chat.editing.alwaysSaveWithGeneratedChanges": true
}
